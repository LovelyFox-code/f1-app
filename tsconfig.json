{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "target": "ES2020",
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "resolveJsonModule": true
    },
    "include": [
        "frontend",
        "backend"
    ]
}